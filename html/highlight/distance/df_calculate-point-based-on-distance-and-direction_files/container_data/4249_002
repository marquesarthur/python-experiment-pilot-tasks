    	var createCDNIFrame = function() {
                var frm = document.getElementById("x2_ad_x2_" + _isn);
                if (!frm) {
                    frm = document.createElement("IFRAME");
                    frm.setAttribute("id","x2_ad_x2_" + _isn);
